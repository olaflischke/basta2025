<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ChickenNhDalNoAutoPks" namespace="ChickenNhDalNoAutoPks">
	<class name="Egg" table="Eggs">
		<id name="Id" column="Id" type="Int32" unsaved-value="0">
			<generator class="identity" />
		</id>
		<property name="Color" column="Color" type="Int32" not-null="true" />
		<property name="Weight" column="Weight" type="Double" not-null="true" />
		<many-to-one name="Mother" column="ChickenId" class="Chicken" foreign-key="none" />
	</class>
</hibernate-mapping>