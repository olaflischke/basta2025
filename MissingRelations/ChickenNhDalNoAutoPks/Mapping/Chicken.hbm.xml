<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ChickenNhDalNoAutoPks" namespace="ChickenNhDalNoAutoPks">
	<class name="Chicken" table="Chickens">
		<id name="Id" column="Id" type="Int32" unsaved-value="0">
			<generator class="identity" />
		</id>
		<property name="Name" column="Name" type="String" not-null="true" />
		<property name="Weight" column="Weight" type="Double" not-null="true" />
		<bag name="Eggs" table="Eggs" cascade="save-update">
			<key column="ChickenId" />
			<one-to-many class="Egg" />
		</bag>
	</class>
</hibernate-mapping>