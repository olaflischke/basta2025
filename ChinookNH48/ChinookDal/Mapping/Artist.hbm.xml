<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ChinookDal" namespace="ChinookDal">
  <class name="Artist">
    <id name="ArtistId">
      <generator class="identity" />
    </id>
    <property name="Name" />
    <bag name="Albums" table="Album">
      <key column="ArtistId" />
      <one-to-many class="Album" />
    </bag>
  </class>
</hibernate-mapping>
