<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ChinookDal" namespace="ChinookDal">
  <class name="Album">
    <id name="AlbumId">
      <generator class="identity" />
    </id>
    <property name="Title" />

    <many-to-one name="Artist" column="ArtistId" cascade="all" class="Artist" />
    
    <bag name="Tracks" table="Track" >
      <key column="AlbumId"/>
      <one-to-many class="Track"/>
    </bag>
  </class>
</hibernate-mapping>
